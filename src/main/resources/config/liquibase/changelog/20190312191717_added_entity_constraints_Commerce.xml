<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity Commerce.
    -->
    <changeSet id="20190312191717-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="product_commerce_id"
                                 baseTableName="commerce"
                                 constraintName="fk_commerce_product_commerce_id"
                                 referencedColumnNames="id"
                                 referencedTableName="product_commerce"/>

        <addForeignKeyConstraint baseColumnNames="offer_id"
                                 baseTableName="commerce"
                                 constraintName="fk_commerce_offer_id"
                                 referencedColumnNames="id"
                                 referencedTableName="offer"/>

        <addForeignKeyConstraint baseColumnNames="owner_id"
                                 baseTableName="commerce"
                                 constraintName="fk_commerce_owner_id"
                                 referencedColumnNames="id"
                                 referencedTableName="user_extra"/>

        <addForeignKeyConstraint baseColumnNames="user_extra_id"
                                 baseTableName="commerce"
                                 constraintName="fk_commerce_user_extra_id"
                                 referencedColumnNames="id"
                                 referencedTableName="user_extra"/>

    </changeSet>
</databaseChangeLog>
